<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:Tabi;assembly=Tabi.Shared" x:Class="Tabi.TabiRaisedButton">
    <ContentView.Content>
        <local:MaterialFrame x:Name="frame" HorizontalOptions="Center" VerticalOptions="Center" BackgroundColor="#2196F3" Padding="10,10,10,10">
            <local:MaterialFrame.GestureRecognizers>
                <TapGestureRecognizer
                Tapped="OnTapGestureRecognizerTapped"
                NumberOfTapsRequired="1" />
            </local:MaterialFrame.GestureRecognizers>
            <Label TextColor="White" Text="Button"/>
        </local:MaterialFrame>
    </ContentView.Content>
</ContentView>